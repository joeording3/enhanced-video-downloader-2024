{
  "name": "enhanced-video-downloader",
  "version": "1.0.0",
  "description": "## Overview Enhanced Video Downloader is a Chrome extension and local Python server (Flask + yt-dlp) for reliable, resumable video downloads from a wide range of sites. It features a draggable download button, popup controls, download history, and robust error handling.",
  "main": "extension/background.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "lint:md": "markdownlint \"**/*.md\" --ignore node_modules",
    "lint:md:fix": "markdownlint \"**/*.md\" --ignore node_modules --fix",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\" --ignore-path .prettierignore",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "build": "npm run build:clean && npm run build:css && npm run build:html && npm run build:ts && npm run build:verify",
    "build:clean": "rm -rf extension/dist",
    "build:lint": "npm run lint:fix && npm run format",
    "build:verify": "npm run build:check-files && npm run build:check-manifest",
    "build:check-files": "node scripts/verify-build.js",
    "build:check-manifest": "node -e \"console.log('Manifest validation:', JSON.parse(require('fs').readFileSync('manifest.json', 'utf8')).manifest_version ? 'PASS' : 'FAIL')\"",
    "build:css": "npm run css:purge && npm run css:minify",
    "build:html": "node scripts/copy-html.js",
    "build:ts": "bash scripts/build-ts.sh",
    "css:purge": "purgecss --css extension/ui/*.css --content extension/ui/*.html extension/src/**/*.ts extension/src/**/*.js --output extension/ui/optimized/",
    "css:minify": "node scripts/minify-css.js",
    "css:optimize": "npm run css:purge && npm run css:minify && npm run css:analyze",
    "css:analyze": "find extension/ui/minified -name '*.css' -exec wc -l {} + | sort -n",
    "analyze:bundle": "node scripts/analyze_bundle_size.js",
    "optimize:tree-shaking": "node scripts/optimize_tree_shaking.js",
    "profile:performance": "node scripts/profile_performance.js",
    "optimize:all": "npm run analyze:bundle && npm run optimize:tree-shaking && npm run profile:performance",
    "test:mutation:js": "stryker run",
    "test:mutation:js:debug": "stryker run --logLevel debug --fileLogLevel trace",
    "test:mutation:js:analyze": "stryker run --reporter html --reporter clear-text",
    "test:playwright": "playwright test",
    "test:playwright:install": "playwright install",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:debug": "playwright test --debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joeording3/Enhanced-Video-Downloader.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joeording3/Enhanced-Video-Downloader/issues"
  },
  "homepage": "https://github.com/joeording3/Enhanced-Video-Downloader#readme",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/preset-env": "^7.27.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@playwright/test": "^1.53.0",
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/jest-runner": "^9.0.1",
    "@stryker-mutator/typescript-checker": "^9.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/chrome": "^0.1.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-istanbul": "^6.1.1",
    "cssnano": "^7.1.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "globals": "^16.1.0",
    "husky": "^8.0.3",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jsonlint": "^1.6.3",
    "lint-staged": "^16.0.0",
    "playwright": "^1.52.0",
    "prettier": "^2.8.8",
    "puppeteer": "^19.0.0",
    "purgecss": "^7.0.2",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "ts-jest": "^29.3.4",
    "v8-to-istanbul": "^9.3.0"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,jsx,tsx}": [
      "eslint --fix --cache --max-warnings=0 --no-warn-ignored",
      "prettier --write"
    ],
    "*.{css,html,json,md}": [
      "prettier --write"
    ],
    "*.md": [
      "python scripts/check_markdown_emojis.py",
      "markdownlint --fix",
      "prettier --write"
    ],
    "server/**/*.py": [
      ".venv/bin/ruff check --fix",
      ".venv/bin/isort --profile black",
      ".venv/bin/black --quiet"
    ],
    "tests/**/*.py": [
      ".venv/bin/ruff check --fix",
      ".venv/bin/isort --profile black",
      ".venv/bin/black --quiet"
    ],
    "extension/src/background-logic.ts": [
      "npm run test:mutation:js:fast"
    ],
    "server/config.py": [
      "npm run test:mutation:py:fast"
    ]
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
